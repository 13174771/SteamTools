    <Color x:Key="BorderColorKey">#FFB9B9C0</Color>
    <SolidColorBrush x:Key="BorderBrushKey" Color="{StaticResource BorderColorKey}" />

    <Color x:Key="ForegroundColorKey">#FF363636</Color>
    <SolidColorBrush x:Key="ForegroundBrushKey" Color="{StaticResource ForegroundColorKey}" />



    <Color x:Key="SemiactiveBackgroundColorKey">#FFF5F5F8</Color>
    <SolidColorBrush x:Key="SemiactiveBackgroundBrushKey" Color="#FFF5F5F8" />

