    <Color x:Key="ThemeColorKey">#AAFFFFFF</Color>
    <SolidColorBrush x:Key="ThemeBrushKey" Color="{StaticResource ThemeColorKey}" />

    <Color x:Key="BackgroundColorKey">#EEF3F3F3</Color>
